<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/salevalueada_lin"
    android:layout_width="match_parent"
    android:layout_height="63dp"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/salevalueada_Tename"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="姓名"
        android:textColor="@color/hei333"
        android:textSize="@dimen/tz" />

    <TextView
        android:id="@+id/salevalueada_Tesz"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="姓名"
        android:textColor="@color/hei333"
        android:textSize="@dimen/tz" />

    <TextView
        android:id="@+id/salevalueada_Teday"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="姓名"
        android:textColor="@color/hei333"
        android:textSize="@dimen/tz" />

    <TextView
        android:id="@+id/salevalueada_Temon"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="姓名"
        android:textColor="@color/hei333"
        android:textSize="@dimen/tz" />
</LinearLayout>
